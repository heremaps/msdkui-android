        <attr name="colorOpacityMaskLight" format="color|reference" />
        <attr name="colorOpacityMask" format="color|reference" />

        <attr name="colorLocation" format="color|reference" />

        <attr name="colorRoute" format="color|reference" />

        <attr name="textTitle" format="reference" />
